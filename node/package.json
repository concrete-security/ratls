{
  "name": "@concrete-security/atlas-node",
  "version": "0.1.0",
  "description": "aTLS client for Node.js - attested fetch for Trusted Execution Environments",
  "type": "module",
  "main": "./atls-fetch.js",
  "types": "./atls-fetch.d.ts",
  "exports": {
    ".": {
      "types": "./atls-fetch.d.ts",
      "import": "./atls-fetch.js"
    },
    "./binding": {
      "types": "./index.d.ts",
      "default": "./index.cjs"
    }
  },
  "napi": {
    "name": "atlas-node",
    "package": {
      "name": "@concrete-security/atlas-node"
    },
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "files": [
    "index.cjs",
    "index.d.ts",
    "atls-fetch.js",
    "atls-fetch.d.ts"
  ],
  "keywords": [
    "atls",
    "tee",
    "tdx",
    "sgx",
    "attestation",
    "confidential-computing",
    "fetch",
    "tls",
    "napi-rs"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/concrete-security/atlas",
    "directory": "node"
  },
  "license": "MIT",
  "scripts": {
    "build": "napi build  --js index.cjs --platform --release",
    "build:debug": "napi build --js index.cjs --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "test": "node test.mjs",
    "sync-versions": "node scripts/sync-versions.mjs"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.76",
    "@napi-rs/cli": "^2.18.4",
    "ai": "^5.0.106",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "optionalDependencies": {
    "@concrete-security/atlas-node-darwin-arm64": "0.1.0",
    "@concrete-security/atlas-node-darwin-x64": "0.1.0",
    "@concrete-security/atlas-node-linux-x64-gnu": "0.1.0",
    "@concrete-security/atlas-node-linux-arm64-gnu": "0.1.0",
    "@concrete-security/atlas-node-win32-x64-msvc": "0.1.0",
    "@concrete-security/atlas-node-win32-arm64-msvc": "0.1.0"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
