{
  "name": "ratls-node",
  "version": "0.1.0",
  "description": "RA-TLS client for Node.js - attested fetch for Trusted Execution Environments",
  "type": "module",
  "main": "./ratls-fetch.js",
  "types": "./ratls-fetch.d.ts",
  "exports": {
    ".": {
      "types": "./ratls-fetch.d.ts",
      "import": "./ratls-fetch.js"
    },
    "./binding": {
      "types": "./index.d.ts",
      "require": "./index.cjs"
    }
  },
  "napi": {
    "name": "ratls-node",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "files": [
    "index.cjs",
    "index.d.ts",
    "ratls-fetch.js",
    "ratls-fetch.d.ts"
  ],
  "keywords": [
    "ratls",
    "tee",
    "tdx",
    "sgx",
    "attestation",
    "confidential-computing",
    "fetch",
    "tls",
    "napi-rs"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zama-ai/secure-chat",
    "directory": "node"
  },
  "license": "MIT",
  "scripts": {
    "build": "bash -c 'napi build --platform --release \"$@\" && mv index.js index.cjs 2>/dev/null || true' --",
    "build:debug": "bash -c 'napi build --platform \"$@\" && mv index.js index.cjs 2>/dev/null || true' --",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "test": "node test.mjs",
    "universal": "napi universal -t @ratls-node"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.76",
    "@napi-rs/cli": "^2.18.4",
    "ai": "^5.0.106",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}